<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tinc.web.dao.ChattingRoomOptionDao">
  <select id="get" resultType="boolean">
 	SELECT * FROM tinc.chattingRoomOption WHERE chattingRoomId=#{chatId} AND memberId=#{memberId}
 </select>
 <insert id="insert">
 	INSERT INTO tinc.chattingRoomOption VALUES (#{chatId},#{memberId})
 </insert>
</mapper>